  "description": "Starter Kit for web development using React, React Router, Redux & Bundling with Webpack ",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./server.js",
    "start:prod": "cross-env NODE_ENV=production node ./server.js",
    "start:lint": "eslint ./src ./cloud --color",
    "build": "cross-env NODE_ENV=production webpack",
    "deploy": "echo \"Error: no deploy specified\" && exit 1",
    "prestart": "npm run start:lint",
    "prebuild": "npm run start:lint",
    "postbuild": "npm run start:prod"
  },
