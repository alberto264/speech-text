{
  "presets": [
    ["env", {
      "debug": true,
      "loose": true,
      "modules": false,
      "useBuiltIns": true,
      "targets": { "browsers": ["last 1 versions", "not ie <=11", "not android <= 56" , "ios >= 10" ] }
    }],
    "react"
  ],
    "plugins": [
      "transform-decorators-legacy",
      ["transform-object-rest-spread", { "useBuiltIns": true }],
      ["transform-class-properties", { "spec": true }]
    ]
}